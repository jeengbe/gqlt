{
  "extends": "./../tsconfig.json",
  "compilerOptions": {
    "outDir": "./../dist",
    "paths": {
      "@paths": [
        "./paths"
      ],
      "@core/*": [
        "./core/*"
      ],
      "@arana/pool/base/*": [
        "./modules/arana/pool/base/*"
      ],
      "@arana/pool/history/*": [
        "./modules/arana/pool/history/*"
      ],
      "@arana/utils/*": [
        "./modules/arana/utils/*"
      ],
      "@modules/*": [
        "./modules/*"
      ],
      "@classes": [
        "./core/classes"
      ]
    },
  },
  "files": [
    "./core/index.ts",
    "./core/scalars.d.ts"
  ],
  "include": [
    "./core/generated/*.json",
    "./modules/**/*.ts"
  ]
}
