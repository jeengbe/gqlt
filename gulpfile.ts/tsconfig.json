{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../gulpfile.js",
    "paths": {
      "@paths": [
        "./paths"
      ],
      "@gulp/*": [
        "./*"
      ],
      "@modules/*": [
        "./../src/modules/*"
      ]
    },
  },
  "include": [
    "**/*",
    "../src/modules/**/gulp/*"
  ]
}
